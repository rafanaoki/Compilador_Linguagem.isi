programa
declare inteiro a, b, i.
declare real x.
declare string nome.
escrevaln("Digite A").
leia(a).
escrevaln("Digite B").
leia(b).
escrevaln("Digite o nome").
leia(nome).
escrevaln("Digite um número real").
leia(x).
for (i := 0. i < b. i := i + 1) {
    escreva("Valor de i: ").
    escrevaln(i).
}
escreva("Valor final de i: ").
escrevaln(i).
while(a < b) {
    a := a + 1.
}
escreva("Valor final de a: ").
escrevaln(a).
escreva("O nome digitado foi: ").
escrevaln(nome).
escreva("O número real digitado foi: ").
escreva(x).
fimprog.
